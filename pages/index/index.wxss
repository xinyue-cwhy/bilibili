page{
  color: #666;
  padding: 0 10rpx;
}


.nav_item{}
.navitems{
  /* 不换行 */
  white-space: nowrap;
  padding: 5rpx 0;
}

.nav{
  padding: 20rpx 45rpx;
  display: inline-block;
  font-size: 30rpx;
}

.active{
  border-bottom: 5rpx solid #de688b;
}

/* 轮播图 */
.swiperItems{
  margin-top: 10rpx;
}
.swiper{
   height: 220rpx;
}
/* 视频 */

.videoimgitem{
  border-radius: 15rpx;
  width: 100%;
}

.video{
  margin-top:10rpx;
  display: flex;
  flex-wrap: wrap;
  padding: 5rpx;
  justify-content: space-between;
}
.videobox{
  width: 48%;
  margin-bottom: 20rpx;
}

.videoimg{
  position: relative;
}

.bofangl{
  position: absolute;
  display: flex;
  flex-wrap: nowrap;
  bottom: 10rpx;
  left: 0;
  width: 100%;
  font-size: 25rpx;
  color: aliceblue;
  justify-content: space-around;
}
.acount{
  font-size: 28rpx;
  display: -webkit-box;
  overflow: hidden;
  white-space: normal;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}


